<h1>X::Home#show</h1>
<p>Find me in app/views/x/home/show.html.erb</p>

<%= form_for @form, url: upload_x_home_path, html: { multipart: true, class: "form-inline" } do |f| %>
	<%= render "common/error", object: @form %>
	<div class="form-group">
	<%= f.file_field :csv_file %>
	</div>
	<%= f.submit "Upload", data: { disable_with: "送信中" }, class: "btn btn-default" %>
<% end %>